.pokemons-main {
  height: 100%;
}
.pokemons-item {
  position: relative;
  padding: 10px 0 10px 10px;
  font-size: 18px;
  padding-right: 10px;
  background: #fff;
  border-bottom: 1px solid #80bd01;
}
.pokemon {
  overflow: hidden;
  padding-top: 5px;
  padding-bottom: 5px;
}
.pokemon-avatar {
  width: 50px;
  height: 50px;
  border-radius: 3px;
  float: left;
}
.pokemon-name {
  color: #666;
  float: left;
  line-height:50px;
  margin-left:5px;
  font-size: 12px;
}
